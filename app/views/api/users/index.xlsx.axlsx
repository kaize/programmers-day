attrs = [:full_name, :email, :phone, :age, :company, :university, :course, :excursio, :subscribe]
xlsx_package.workbook.add_worksheet(name: 'sheet1') do |sheet|
  style_bold = sheet.workbook.styles.add_style b: true
  sheet.add_row attrs.map { |c| User.human_attribute_name(c) }, style: style_bold
  @users.each do |user|
    sheet.add_row attrs.map { |a| user.send(a).to_s }
  end
end
